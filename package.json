{
	"devDependencies": {
		"@eslint/js": "^9.30.1",
		"eslint": "^9.30.1",
		"husky": "^9.1.7",
		"lint-staged": "^16.1.2",
		"prettier": "3.6.2",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.35.1"
	},
	"scripts": {
		"prepare": "husky",
		"lint": "prettier --check .",
		"format": "prettier --write ."
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"**/*": "prettier --write --ignore-unknown"
	}
}
